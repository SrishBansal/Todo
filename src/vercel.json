{
    "version": 2,
    "platforms": ["@vercel/node"],
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "output" : "/"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}